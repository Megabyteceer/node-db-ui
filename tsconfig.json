{
	"compilerOptions": {
		"noUnusedLocals": true,
		"moduleResolution": "nodenext",
		"lib": [
			"DOM",
			"ESNext"
		],
		"target": "esnext",
		"rootDir": ".",
		"baseUrl": ".",
		"sourceMap": true,
		"outDir": "build-debug",
		"incremental": true,
		"module": "nodenext",
		"diagnostics": false,
		"typeRoots": [
			"types"
		],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"assumeChangesOnlyAffectDirectDependencies": true,
	},
	"include": [
		"www",
		"types"
	],
	"exclude": [
		"node_modules",
		"**/build-prod/**",
		"**/build-debug/**",
		"**/uploads/**"
	]
}