{
	"compilerOptions": {
		"noUnusedLocals": true,
		"moduleResolution": "nodenext",
		"lib": [
			"DOM",
			"ESNext",
			"ES2024.String"
		],
		"noImplicitAny": true,
		"target": "esnext",
		"rootDir": ".",
		"baseUrl": ".",
		"sourceMap": true,
		"outDir": "build-debug",
		"incremental": true,
		"strict": true,
		"module": "nodenext",
		"diagnostics": false,
		"types": [],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"assumeChangesOnlyAffectDirectDependencies": true,
	},
	"exclude": [
		"node_modules",
		"**/build-prod/**",
		"**/build-debug/**",
		"www/uploads/**",
		"data/**"
	]
}