{
	"compilerOptions": {
		"target": "es2020",
		"rootDir": ".",
		"baseUrl": ".",
		"paths": {
			"/*": [
				"./*"
			]
		},
		"outDir": "",
		"module": "es2020",
		"moduleResolution": "node",
		"diagnostics": false,
		"skipLibCheck": true,
		"checkJs": true
	},
	"exclude": [
		"node_modules",
		"**/node_modules/*",
		"**/node_modules/**",
		"**/uploads/**",
	]
}